<body>
    <script>
        let ID = prompt("Nhập tên người dùng:");
        if (ID === "ADMIN") {
            let pw = prompt("Nhập mật khẩu:");
            if (pw === "TheMaster") {
                document.write("Welcome");
            } else if (pw === "") {
                document.write("Cancelled");
            } else {
                document.write("Wrong password");
            }
        } else if (ID === "") {
            document.write("Cancelled");
        } else {
            document.write("I Don’t know you");
        }
    </script>
</body>
